  <eClassifiers xsi:type="ecore:EEnum" name="FindingSeverity">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Enumeration of severity levels for a compliance finding."/>
    </eAnnotations>
    <eLiterals name="INFO">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Informational finding that does not indicate a problem but provides context."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="WARNING" value="1">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A potential issue or a minor deviation from the policy that should be reviewed."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="ERROR" value="2">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A definite violation of the policy that must be addressed."/>
      </eAnnotations>
    </eLiterals>
    <eLiterals name="CRITICAL" value="3">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A severe violation that poses a significant risk and requires immediate attention."/>
      </eAnnotations>
    </eLiterals>
  </eClassifiers>
