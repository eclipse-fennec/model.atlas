      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Human-readable name of this dependency (e.g., 'MQTT Data Inbound', 'SSL Security')"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Detailed description of what this dependency represents"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="referencedComponent" eType="#//SystemComponent"
        eKeys="#//SystemComponent/componentId">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Optional reference to the specific system component this dependency is connected to"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="governanceNamespace" eType="#//GovernanceNamespace"
        eKeys="#//GovernanceNamespace/namespaceId">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Reference to the governance namespace this dependency operates within"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Requirement" eSuperTypes="#//ComponentDependency">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Represents what a system component needs from other components within a governance namespace. Requirements are fulfilled by matching Capabilities from other components."/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="capability" upperBound="-1"
        eType="#//Capability" eOpposite="#//Capability/requirement">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
